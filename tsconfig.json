{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["nativewind/types"],
    "strict": true,
    "esModuleInterop": true, // Thêm dòng này
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "app/(tabs)/profile.jsx", "app/index.jsx", "i18n.js"]
}
